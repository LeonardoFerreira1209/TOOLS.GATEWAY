				"ClientWhitelist": [],
				"EnableRateLimiting": true,
				"Period": "1m",
				"PeriodTimespan": 60,
				"Limit": 10
			}
		}
	],
	"GlobalConfiguration": {
		"BaseUrl": "https://localhost:7282",
		"RateLimitOptions": {
			"DisableRateLimitHeaders": false,
