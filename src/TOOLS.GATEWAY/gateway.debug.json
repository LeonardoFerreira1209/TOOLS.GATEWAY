				"ClientWhitelist": [],
				"EnableRateLimiting": true,
				"Period": "1m",
				"PeriodTimespan": 60,
				"Limit": 10
			}
		},
		{
			"DownstreamPathTemplate": "/api/user/get/{userId}", // Url da API.
			"DownstreamScheme": "https",
			"DownstreamHostAndPorts": [
				{
					"Host": "localhost",
					"Port": 7125
				}
			],
			"UpstreamPathTemplate": "/gateway/user/get/{userId}", // Url do Gateway
			"UpstreamHttpMethod": [ "GET" ], // Methos aceitos.
			"RouteIsCaseSensitive": false,
			"DownstreamHttpMethod": "GET", // Method da API.
